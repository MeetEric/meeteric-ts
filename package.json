{
  "name": "meeteric-ts",
  "version": "0.2.3",
  "main": "./bundle",
  "scripts": {
    "build": "tsc",
    "build-test": "npm run test",
    "clean": "gts clean",
    "gts-fix": "gts fix",
    "lint": "gts lint",
    "test": "mocha",
    "prebuild-test": "npm run build",
    "test-log": "npm run build && node release/test/RunLog.js",
    "test-withreport": "mocha --recursive release/**/*Test*.js --reporter mocha-junit-reporter --reporter-options mochaFile=./testresults/test-results.xml",
    "copy-package": "copyfiles package.json ./release/",
    "package": "npm run copy-package && npm pack ./release"
  },
  "author": {
    "url": "https://www.meeteric.net",
    "name": "Eric Maino",
    "email": "eric.maino@microsoft.com"
  },
  "description": "A collection of strongly typed context neutral contracts and implementations",
  "repository": {
    "type": "git",
    "url": "https://github.com/MeetEric/meeteric-ts"
  },
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.4.3",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "ajv": "^6.1.1",
    "ajv-keywords": "^3.1.0",
    "chai": "^4.3.4",
    "copyfiles": "^2.1.0",
    "del": "^3.0.0",
    "gts": "^3.1.0",
    "mocha": "^9.0.3",
    "mocha-junit-reporter": "^1.18.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
